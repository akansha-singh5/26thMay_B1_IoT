//sa re ga ma generator

void setup()
{
  pinMode(12, OUTPUT);
}

void loop()
{
  tone(12, 240, 1000); 
  delay(1000); 
  tone(12, 270, 1000); 
  delay(1000); 
  tone(12, 300, 1000); 
  delay(1000); 
  tone(12, 320, 1000); 
  delay(1000); 
  tone(12, 360, 1000); 
  delay(1000); 
  tone(12, 400, 1000); 
  delay(1000); 
  tone(12, 450, 1000); 
  delay(1000); 
  
}
